{
  "name": "seworqs/commons-i18n",
  "description": "Easy to use translations for your PHP project.",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": ["seworqs", "language", "i18n", "translation"],
  "version": "2.1.0",
  "authors": [
    {
      "name": "S.E. Franke",
      "email": "sven.franke@seworqs.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seworqs\\Commons\\I18n\\": "src/"
    },
    "files": [
      "src/Functions/TranslationFunctions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Seworqs\\Commons\\I18n\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-intl": "*",
    "laminas/laminas-i18n": "^2.30"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "phpunit"
  }
}
